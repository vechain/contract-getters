{
  "name": "@vechain/react-hooks",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --noEmit && cross-env NODE_OPTIONS='--max-old-space-size=8192' tsup",
    "watch": "cross-env NODE_OPTIONS='--max-old-space-size=8192' tsup --watch",
    "clean": "rm -rf dist .turbo",
    "lint": "eslint",
    "purge": "yarn clean && rm -rf node_modules",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@privy-io/react-auth": "2.8.0",
    "@solana/web3.js": "^1.90.0",
    "@tanstack/react-query": "^5.64.2",
    "@vechain/connex": "^2.0.0",
    "@vechain/picasso": "^2.1.1",
    "@vechain/sdk-network": "1.1.0",
    "@vechain/vebetterdao-contracts": "^4.1.0",
    "@wagmi/core": "^2.17.2",
    "bignumber.js": "^9.1.2",
    "browser-image-compression": "^2.0.2",
    "ethers": "^6.13.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "thor-devkit": "^2.0.0",
    "viem": "^2.29.3",
    "wagmi": "^2.15.4"
  },
  "peerDependencies": {
    "@vechain/dapp-kit-react": "2.0.0-rc.1"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.4.0",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.28",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typechain": "^8.3.0",
    "typescript": "*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./contracts": {
      "types": "./dist/contracts/index.d.ts",
      "default": "./dist/contracts/index.js"
    }
  }
}
