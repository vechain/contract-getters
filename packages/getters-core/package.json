{
  "name": "@vechain/getters-core",
  "version": "0.0.1",
  "private": false,
  "description": "Framework-agnostic VeChain getters (read-only).",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --sourcemap",
    "dev": "tsup src/index.ts --dts --format esm,cjs --sourcemap --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo \"(add eslint later)\"",
    "test": "echo \"(add tests later)\"",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@vechain/sdk-network": "^2.0.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.10",
    "@vechain/sdk-network": "^2.0.3",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4"
  }
}
