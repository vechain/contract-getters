{
    "name": "@vechain/contract-getters",
    "version": "1.0.0-beta.0",
    "homepage": "https://github.com/vechain/contract-getters",
    "description": "Framework-agnostic VeChain getters (read-only).",
    "repository": "github:vechain/contract-getters",
    "license": "MIT",
    "sideEffects": false,
    "module": "dist/index.mjs",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/**/*.ts",
        "clean": "rm -rf dist"
    },
    "peerDependencies": {
        "@vechain/sdk-network": "^2.0.3",
        "@vechain/vebetterdao-contracts": "^7.0.2",
        "ethers": "^6.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^8.40.0",
        "@typescript-eslint/parser": "^8.40.0",
        "@vechain/sdk-network": "^2.0.3",
        "@vechain/vebetterdao-contracts": "^7.0.2",
        "ethers": "^6.0.0",
        "tsup": "^8.5.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.40.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    }
}
