{
    "name": "nodejs-getters-example",
    "version": "0.0.1",
    "private": true,
    "description": "Node.js example showcasing VeChain getters usage",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "start": "yarn build && node dist/index.js",
        "dev": "tsx watch src/index.ts",
