        "@vechain/sdk-network": "^2.0.3"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "eslint": "^9.33.0",
        "tsx": "^4.7.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.40.0"
    }
}
